   "source": [
    "class Solution:\n",
    "    def arraySign(self, nums: List[int]) -> int:\n",
    "        num0 = nums[0]\n",
    "        for n in nums[1:]:\n",
    "            num0 *= n\n",
    "        \n",
    "        if num0 > 0:\n",
    "            return 1\n",
    "        elif num0 < 0:\n",
    "            return -1\n",
    "        else:\n",
    "            return 0"
   ]
  }
